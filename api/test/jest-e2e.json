{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts",
    "tsx"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(tsx?|js)$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/../src/$1"
  },
  "setupFiles": [
    "<rootDir>/setupEnvVars.js"
  ]
}
